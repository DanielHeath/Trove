(function(){tinymce.PluginManager.requireLangPack('image_upload');tinymce.create('tinymce.plugins.ImageUploadPlugin',{init:function(ed,url){ed.addCommand('mceImageUpload',function(){ed.windowManager.open({file:url+'/dialog.htm',width:320+parseInt(ed.getLang('image_upload.delta_width',0)),height:120+parseInt(ed.getLang('image_upload.delta_height',0)),inline:1},{plugin_url:url,some_custom_arg:'custom arg'})});ed.addButton('image_upload',{title:'image_upload.desc',cmd:'mceImageUpload',image:url+'/img/image_upload.gif'});ed.onNodeChange.add(function(ed,cm,n){cm.setActive('image_upload',n.nodeName=='IMG')})},createControl:function(n,cm){return null},getInfo:function(){return{longname:'ImageUpload plugin',author:'Some author',authorurl:'http://tinymce.moxiecode.com',infourl:'http://wiki.moxiecode.com/index.php/TinyMCE:Plugins/image_upload',version:"1.0"}}});tinymce.PluginManager.add('image_upload',tinymce.plugins.ImageUploadPlugin)})();
